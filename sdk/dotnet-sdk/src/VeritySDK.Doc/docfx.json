{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\VeritySDK"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [],
        "exclude": [
          "obj/**"
        ]
      }
    ],
    "dest": "../../doc",
    "globalMetadata": {
      "_appTitle": "VeritySDK .Net",
      "_appFooter": "<span>Copyright Â© Evernym, Inc.<br>Generated by <strong>DocFX</strong></span>",
      "_gitContribute": {
        "branch": "dev"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false
  }
}